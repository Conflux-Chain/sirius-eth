{
  "github": {
    "silent": true
  },
  "routes": [
    {
      "src": "/v1/(?<req>.*)",
      "dest": "https://scantest.confluxnetwork.org/v1/$req"
    },
    {
      "src": "/stat/(?<req>.*)",
      "dest": "https://scantest.confluxnetwork.org/stat/$req"
    },
    {
      "src": "/rpc",
      "dest": "https://scantest.confluxnetwork.org/rpc"
    }
  ]
}
